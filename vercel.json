{
  "buildCommand": "npm run build",
  "outputDirectory": "packages/wallet-ui/dist",
  "rewrites": [
    {
      "source": "/api/health",
      "destination": "/api/health"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/serverless"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
