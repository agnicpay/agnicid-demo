{
  "name": "@agnicid/wallet-ui",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    "./server": "./dist-server/index.js",
    "./server/api": "./dist-server/createWalletApi.js"
  },
  "scripts": {
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:vite\"",
    "dev:vite": "vite --host 127.0.0.1 --port 5173",
    "dev:frontend": "vite --host 127.0.0.1 --port 5173",
    "dev:server": "concurrently \"npm:watch:server\" \"npm:serve:server\"",
    "dev:api": "concurrently \"npm:watch:server\" \"npm:serve:server\"",
    "watch:server": "tsc -b tsconfig.server.json --watch --preserveWatchOutput",
    "serve:server": "node ./scripts/start-dev-server.mjs",
    "build": "tsc -b tsconfig.json && vite build && tsc -b tsconfig.server.json",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "dependencies": {
    "@agnicid/agent-sdk": "file:../agent-sdk",
    "@agnicid/issuer-cli": "file:../issuer-cli",
    "adm-zip": "^0.5.12",
    "@headlessui/react": "^1.7.18",
    "axios": "^1.6.8",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "clsx": "^2.1.0",
    "idb-keyval": "^6.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.22",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/adm-zip": "^0.5.4",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.7.2",
    "vite": "^5.1.4"
  }
}
